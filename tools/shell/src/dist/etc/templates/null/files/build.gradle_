
version = 1.0

rootProject.ext {
  zestVersion = @version@
}

allprojects() {
  apply plugin: 'java'
  apply plugin: 'maven'

  repositories {
    mavenLocal()
    mavenCentral()
  }

  dependencies {
    compile "org.apache.zest.core:org.apache.zest.core.api:$zestVersion"
    testCompile "org.apache.zest.core:org.apache.zest.core.testsupport:$zestVersion"
  }
}

